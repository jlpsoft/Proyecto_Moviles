<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.julioleonardo.inventarline">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Cpanel" android:label="Panel de Control">
        </activity>
        <activity android:name=".AddProduct" android:label="Agregar Producto">
        </activity>
        <activity android:name=".ListingProduct" android:label="Listar Producto">
        </activity>
        <activity android:name=".ModifyProduct" android:label="Modificar Producto">
        </activity>
        <activity android:name=".DeleteProduct" android:label="Eliminar Producto">
        </activity>
        <activity android:name=".GenerateReport" android:label="Generar Reporte">
        </activity>
    </application>
</manifest>
